{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts?$",
  "testEnvironment": "node",
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "collectCoverage": true,
  "coverageReporters": [
    "json",
    "text",
    "html"
  ],
  "coverageDirectory": "reports"
}
